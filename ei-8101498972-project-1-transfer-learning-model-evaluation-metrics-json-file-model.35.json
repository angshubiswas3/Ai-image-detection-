def print_section(title, model):
    print("\n======================")
    print(title)
    print("======================")
    print(f"Accuracy        : {model['accuracy'] * 100:.2f}%")
    print(f"ROC AUC         : {model['roc_auc']:.4f}")
    print(f"Loss            : {model['loss']:.4f}")
    print("Confusion Matrix:")
    for row in model["confusion_matrix"]:
        print(" ", row)

print_section("INT8 Model Performance", data["int8"])
print_section("Float32 (Training) Performance", data["float32_train"])
print_section("Float32 (Test) Performance", data["float32_test"])
